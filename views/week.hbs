{{#section 'head'}}
  <script src="js/week.js" type="text/javascript"></script>
  <link rel="stylesheet" href="/css/week.css">
{{/section}}

{{#> layout }}

  {{> headerplanner}}

  <div class="content">
    <div class="week-container">
      <div class="week-header">
        <div class="week-datename">Juni 2018, Lørdag</div>
        <div class="week-days">
          <ul>
            <h3 class="this-week">Uke</h3>
            {{#each daysstring}}
              <li>{{this}}</li>
            {{/each}}
            <h3 class="this-week">{{uke}}</h3>
            {{#each daysnum}}
              <li>{{this}}</li>
            {{/each}}
          </ul>
        </div>
      </div>
      <div class="week-list">
        <div class="list-item">
          {{#each daysstring}}
            <li id="vare">{{this}}</li>
          {{/each}}
        </div>
        <div class="item-button">
          <img id="button" src="/imgs/icon/Varer.png" alt="">
        </div>
      </div>
    </div>
  </div>
